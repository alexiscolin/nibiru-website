{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
        "source": "**/*",
        "headers": [ {
          "key": "X-Robots-Tag",
          "value": "noindex"
        } ]
      }]
  }
}
